	<!--     配置Chinese实例，其实现类为com.systop.bean.ChineseImpl -->
	<bean id="Chinese" class="com.systop.bean.ChineseImpl">
		<!-- 	将值注入name属性 -->
		<property name="name">
			<value>小明</value>
		</property>
		<!-- 	将值注入age属性 -->
		<property name="age">
			<value>18</value>
		</property>
	</bean>
	<!-- 	配置American实例，其实现类为com.systop.bean.AmericanImpl -->
